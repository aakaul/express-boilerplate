{
    "name": "express-boiler-plate",
    "version": "1.0.0",
    "description": "This is express boiler plate with CQRS principle ",
    "main": "index.js",
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "scripts": {
        "start": "node ./dist",
        "devtsc": "NODE_ENV=development && npx tsc-watch --onSuccess \"node ./dist/index.js\"",
        "g": "npx ts-node  ./src/templates/scripts/Generate.ts",
        "check": "tsc --build tsconfig.prod.json",
        "views-watch": "chokidar \"src/**/*.hbs\" \"src/public/**/*.*\" -c \"rm -R dist/views && cp -R src/views dist/  && rm -R dist/public && cp -R src/public dist/\"",
        "dev": "concurrently \"npm run devtsc\" \"npm run views-watch\"",
        "test": "jest",
        "build": "node ./utils/build.js",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/config/typeOrm.config.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/aakaul/express-boiler-plate.git"
    },
    "author": "Abhey",
    "license": "MIT",
    "bugs": {
        "url": "https://gitlab.com/aakaul/express-boiler-plate/issues"
    },
    "homepage": "https://gitlab.com/aakaul/express-boiler-plate#readme",
